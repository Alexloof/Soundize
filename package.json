{
  "name": "soundize",
  "version": "0.1.0",
  "private": true,
  "description": "A client-side webapp powered by Spotify API",
  "author": "Alex Lööf",
  "license": "ISC",
  "main": "dist/server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Alexloof/Soundize.git"
  },
  "scripts": {
    "start": "node ./dist/server.js",
    "clean": "rm -rf ./dist",
    "build": "webpack --config ./webpack.prod.config.js --progress --colors",
    "dev": "npm-run-all --parallel dev:*",
    "dev:build-server": "webpack --config webpack.devserver.config.js --watch",
    "dev:build-client": "webpack --config webpack.dev.config.js --watch",
    "dev:server": "nodemon --exec node dist/server.js",
    "prod:build-server": "webpack --config webpack.devserver.config.js",
    "prod:build-client": "webpack --config webpack.dev.config.js"
  },
  "dependencies": {
    "autoprefixer": "^6.4.1",
    "axios": "^0.16.2",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.16.0",
    "css-loader": "^0.26.1",
    "express": "^4.15.4",
    "express-session": "^1.15.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "nodemon": "^1.12.1",
    "prop-types": "^15.5.10",
    "querystring": "^0.2.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-flip-move": "^2.9.15",
    "react-player": "^0.21.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "react-s-alert": "^1.3.2",
    "react-scripts": "1.0.12",
    "redux": "^3.7.2",
    "redux-persist": "^5.3.4",
    "redux-thunk": "^2.2.0",
    "screenfull": "^3.3.1",
    "serialize-javascript": "^1.4.0",
    "spotify-web-api-node": "^2.4.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.6.2"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "file-loader": "^0.11.2",
    "image-webpack-loader": "^3.4.2",
    "json-loader": "^0.5.7",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.2",
    "sass-loader": "^6.0.6",
    "webpack": "^2.7.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.4.1",
    "webpack-node-externals": "^1.6.0"
  }
}
